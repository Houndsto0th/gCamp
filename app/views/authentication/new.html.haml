.container
  .well.col-sm-4.col-sm-offset-4
    %h1 Sign into gCamp

    = form_tag signin_path do |f|

      .form-group
        = label_tag :email
        = email_field_tag :email
      .form-group
        = label_tag :password
        = password_field_tag :password
      .form-group
        = submit_tag "Sign in", class: "btn btn-primary"
